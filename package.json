{
  "name": "adocavo-net",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "analyze:bundles": "ANALYZE=true npm run build",
    "build:open-next": "opennextjs-cloudflare build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "deploy:preview": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env preview",
    "deploy:backup": "wrangler deploy --config wrangler.backup.toml",
    "dev:backup": "wrangler dev --config wrangler.backup.toml",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "wrangler d1 migrations apply adocavo-db --remote",
    "db:migrate:local": "wrangler d1 migrations apply adocavo-db --local",
    "seed:hooks": "tsx scripts/seed-hooks.ts",
    "seed:review-queue": "tsx scripts/import-review-queue.ts",
    "seed:all": "tsx scripts/seed.ts",
    "seed:niche-examples": "tsx scripts/generate-niche-examples.ts",
    "test": "vitest run",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts --passWithNoTests",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "perf:budget": "npm run build && lhci autorun --config=./lighthouserc.json",
    "validate:env": "bash scripts/validate-env.sh",
    "setup:secrets": "bash scripts/setup-secrets.sh",
    "rollback": "bash scripts/rollback.sh",
    "notify:deployment": "bash scripts/notify-deployment.sh",
    "monitor:deployment": "bash scripts/monitor-deployment.sh",
    "extract:blog": "tsx scripts/extract-blog-content.ts"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "1.7.0",
    "@opennextjs/cloudflare": "1.14.8",
    "@radix-ui/react-collapsible": "1.1.0",
    "@radix-ui/react-dialog": "1.1.0",
    "@radix-ui/react-dropdown-menu": "2.1.0",
    "@radix-ui/react-label": "2.1.0",
    "@radix-ui/react-radio-group": "1.2.0",
    "@radix-ui/react-slot": "1.1.0",
    "@radix-ui/react-toast": "1.2.0",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "date-fns": "3.6.0",
    "drizzle-orm": "0.38.0",
    "html2canvas": "1.4.1",
    "isomorphic-dompurify": "2.35.0",
    "jspdf": "4.0.0",
    "lucide-react": "0.468.0",
    "nanoid": "5.0.8",
    "next": "15.1.11",
    "next-auth": "5.0.0-beta.25",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-markdown": "9.0.1",
    "remark-gfm": "4.0.0",
    "sonner": "2.0.7",
    "tailwind-merge": "2.6.0",
    "zod": "3.24.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20260109.0",
    "@lhci/cli": "0.13.0",
    "@next/bundle-analyzer": "15.1.11",
    "@playwright/test": "1.57.0",
    "@tailwindcss/typography": "0.5.15",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/dompurify": "3.2.0",
    "@types/node": "22.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitest/coverage-v8": "^2.1.9",
    "autoprefixer": "10.4.20",
    "drizzle-kit": "0.30.0",
    "eslint": "9.16.0",
    "eslint-config-next": "15.1.11",
    "happy-dom": "^15.11.0",
    "postcss": "8.4.49",
    "tailwindcss": "3.4.16",
    "tsx": "4.19.2",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "2.1.9",
    "wrangler": "4.58.0"
  },
  "overrides": {
    "esbuild": "0.27.0"
  }
}
